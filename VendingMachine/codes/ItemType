package item;

public enum ItemType {
    OREO("OREO"),//i=0,8
    CHIPS("CHIPS"),//i=1,9
    CHOCOLATE("CHOCOLATE"),//i=2
    COKE("COKE"),//i=3
    SODA("SODA"),//i=4
    GUM("GUM"),//i=5
    CANDY("CANDY"),//i=6
    JUICE("JUICE");//i=7
    private final String type;
     ItemType(String type){
        this.type=type;
    }
     public String getItem(){
        return type;
     }
}
